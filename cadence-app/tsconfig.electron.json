{
    "compilerOptions": {
      "target": "ES2020",
      "module": "CommonJS",
      "outDir": "dist-electron",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true
    },
    "include": ["electron/**/*.ts"],
    "exclude": ["node_modules"]
  }